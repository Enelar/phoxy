<%= form_tag('#', {"id": "reg_form"}) %>
<table>
<%
for (var name in form)
{
  if (name === 'length' || !form.hasOwnProperty(name)) continue;
  var type = form[name];
  var value = '';
  if (values.hasOwnProperty(name))
    value = values[name];
   %>
<tr><td>
<% if (type != 'hidden')
   {
   %>
<%= name %>
<% } %>
</td><td>
<%= input_field_tag(name, value, type, {"requered":"true"}) %>
</td></tr>

<%
}
%>
<tr><td></td><td><%= submit_tag('Register') %><%= link_to('Login', 'javascript:PrefferLogin()') %></td></tr>
</table>
</form>
